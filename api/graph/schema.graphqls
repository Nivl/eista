type Health {
  status: String!
}

type Me {
  id: ID!
  name: String!
  email: String!
}

type Query {
  health: Health!
  me: Me
}

input NewUser {
  name: String!
  email: String!
  password: String!
}

input Credentials {
  email: String!
  password: String!
}

type Session {
  token: String!
}

type Mutation {
  createUser(userData: NewUser!): Boolean!
  signIn(credentials: Credentials!): Session
}
